<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="670.0" prefWidth="920.0" stylesheets="@customers.css" xmlns="http://javafx.com/javafx/9.0.4"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.blackandwhite.grey.customers.ControllerCustomers">
    <children>
        <Label layoutX="25.0" layoutY="20.0" styleClass="title" text="Customers"/>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="200.0" layoutX="30.0" layoutY="85.0"
                   onMouseClicked="#add" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0"
                   width="250.0"/>
        <Label layoutX="45.0" layoutY="100.0" text="Add New">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="200.0" layoutX="325.0" layoutY="85.0"
                   onMouseClicked="#search" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0"
                   width="250.0"/>
        <Label layoutX="340.0" layoutY="100.0" text="Search">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="300.0" layoutX="30.0" layoutY="325.0"
                   stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="860.0"/>
        <TableView fx:id="table" layoutX="35.0" layoutY="339.0" prefHeight="250.0" prefWidth="850.0">
            <columns>
                <TableColumn fx:id="firstCol" prefWidth="75.0" text="First name"/>
                <TableColumn fx:id="lastCol" prefWidth="75.0" text="Last name"/>
                <TableColumn fx:id="emailCol" prefWidth="75.0" text="Email"/>
                <TableColumn fx:id="workCol" prefWidth="75.0" text="Work #"/>
                <TableColumn fx:id="cellCol" prefWidth="75.0" text="Cell #"/>
                <TableColumn fx:id="homeCol" prefWidth="75.0" text="Home #"/>
                <TableColumn fx:id="addrCol" prefWidth="75.0" text="Address"/>
                <TableColumn fx:id="cityCol" prefWidth="75.0" text="City"/>
                <TableColumn fx:id="provCol" prefWidth="75.0" text="Province"/>
                <TableColumn fx:id="postalCol" prefWidth="75.0" text="Postal Code"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
