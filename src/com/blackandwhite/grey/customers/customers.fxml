<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="920.0" stylesheets="@customers.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.blackandwhite.grey.customers.ControllerCustomers">
    <children>
        <Label layoutX="25.0" layoutY="20.0" styleClass="title" text="Customers" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="200.0" layoutX="30.0" layoutY="85.0" onMouseClicked="#add" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="250.0" />
        <Label layoutX="45.0" layoutY="100.0" text="Add New">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="200.0" layoutX="325.0" layoutY="85.0" onMouseClicked="#search" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="250.0" />
        <Label layoutX="340.0" layoutY="100.0" text="Search">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="350.0" layoutX="30.0" layoutY="325.0" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="860.0" />
        <TableView fx:id="table" layoutX="35.0" layoutY="339.0" prefHeight="325.0" prefWidth="850.0">
            <columns>
                <TableColumn fx:id="colFirstName" maxWidth="300.0" minWidth="50.0" prefWidth="150.0" text="First name" />
                <TableColumn fx:id="colLastName" maxWidth="300.0" minWidth="50.0" prefWidth="150.0" text="Last name" />
                <TableColumn fx:id="colEmail" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Email" />
                <TableColumn fx:id="colWorkPhone" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Work #" />
                <TableColumn fx:id="colCellPhone" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Cell #" />
                <TableColumn fx:id="colHomePhone" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Home #" />
                <TableColumn fx:id="colAddress" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Address" />
                <TableColumn fx:id="colCity" maxWidth="300.0" minWidth="50.0" prefWidth="125.0" text="City" />
                <TableColumn fx:id="colProvince" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Province" />
                <TableColumn fx:id="colPostalCode" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Postal Code" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
