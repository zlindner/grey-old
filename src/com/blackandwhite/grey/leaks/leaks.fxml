<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="720.0" prefWidth="920.0" stylesheets="@leaks.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.blackandwhite.grey.leaks.ControllerLeaks">
    <children>
        <Label layoutX="25.0" layoutY="20.0" styleClass="title" text="Leaks" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="350.0" layoutX="30.0" layoutY="315.0" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="860.0" />
      <TableView fx:id="table" layoutX="35.0" layoutY="325.0" prefHeight="335.0" prefWidth="850.0">
         <columns>
            <TableColumn fx:id="colStatus" maxWidth="300.0" minWidth="50.0" prefWidth="100.0" text="Status" />
            <TableColumn fx:id="colAddress" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Address" />
            <TableColumn fx:id="colOpened" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Opened" />
            <TableColumn fx:id="colClosed" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Closed" />
            <TableColumn fx:id="colAssigned" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" text="Assigned" />
         </columns>
      </TableView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="100.0" layoutX="30.0" layoutY="85.0" stroke="WHITE" strokeLineCap="ROUND" strokeType="OUTSIDE" strokeWidth="5.0" width="500.0" />
      <TextField fx:id="tfAddress" layoutX="40.0" layoutY="120.0" prefHeight="30.0" prefWidth="250.0" promptText="Address" />
      <Button layoutX="315.0" layoutY="120.0" mnemonicParsing="false" onAction="#open" prefHeight="30.0" prefWidth="200.0" text="Open Leak" />
    </children>
</AnchorPane>
